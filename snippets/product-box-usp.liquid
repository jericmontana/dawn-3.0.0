    Accepts:
    - icon

    Usage:
    {% render 'product-box-usp',
        icon: section.settings.icon
    %}

    Schema:
    {
        "settings": [

            ...
                {
                    "type": "select",
                    "id": "usp_icon",
                    "label": "Icon",
                    "options": [
                        {
                        "value": "money-back-guarantee",
                        "label": "Money Back Guarantee"
                        },
                        {
                        "value": "easy-to-apply",
                        "label": "Easy to apply"
                        },
                        {
                        "value": "handmade",
                        "label": "Handmade"
                        },
                        {
                        "value": "lightweight",
                        "label": "Lightweight"
                        },
                        {
                        "value": "longlasting",
                        "label": "Longlasting"
                        },
                        {
                        "value": "multiple-use",
                        "label": "Multiple Use"
                        },
                        {
                        "value": "natural",
                        "label": "Natural"
                        }
                    ],
                    "default": "natural"
                }
            ...

        ]
    }

{% endcomment %}

